<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.pelisalacarta"
    name="pelisalacarta"
    version="4.2.0~final"
    provider-name="tvalacarta">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.libtorrent" optional="true"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="es">Películas y series a la carta</summary>
    <description lang="es">Películas, series, documentales, anime, ... Todo está en Internet, y ahora en tu media center.</description>
    <summary lang="en">Movies and TV Shows on demand</summary>
    <description lang="en">Movies, Series, Documentaries, Anime, ... Everything is on the net, and now on your media center.</description>
    <platform>all</platform>
    <license>GNU GPL v3</license>
    <forum>http://www.mimediacenter.info/foro</forum>
    <website>http://blog.tvalacarta.info/plugin-xbmc/pelisalacarta</website>
    <email>tvalacarta@gmail.com</email>
    <source>https://github.com/tvalacarta/pelisalacarta</source>
  </extension>
  <extension point="xbmc.service" library="library_service.py" start="login|startup">
  </extension>
</addon>

<addon
	id="plugin.video.arenavisionezy"
	name="ArenaVision EZY"
	version="5.3.8"
	provider-name="Wazzu">
	<requires>
		<import addon="xbmc.python" version="2.1.0"/>
		<import addon="program.plexus" />
	</requires>
	<extension point="xbmc.python.pluginsource" library="addon.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>ArenaVision EZY</summary>
		<description>Interfaz para ArenaVision</description>
		<platform>all</platform>
		<license/>
		<forum/>
		<website/>
		<email>egrueda@gmail.com</email>
		<source/>
	</extension>
	<extension point="xbmc.addon.repository" name="ArenaVision EZY">
		<info compressed="false">https://avezy.tk/repo/addons.xml</info>
		<checksum>https://avezy.tk/repo/addons.xml.md5</checksum>
		<datadir zip="true">https://avezy.tk/repo/zip/</datadir>
	</extension>
</addon>


<addon id="repository.llaurator" name="llaurator official Repo" version="1.0.0" provider-name="Llaurator">
	<extension point="xbmc.addon.repository" name="llaurator official Repo">
		<info compressed="false">https://raw.githubusercontent.com/llaurator/llaurator-repo/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/llaurator/llaurator-repo/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/llaurator/llaurator-repo/master</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>llaurator official Repo</summary>
   		<description lang="en">Instala los Addons recomendados por llaurator.</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
